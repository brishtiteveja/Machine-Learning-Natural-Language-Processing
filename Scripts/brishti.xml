  <rdf:Description rdf:about="http://sead-test/fakeUri/0489a707-d428-4db4-8ce0-1ace548bc653">
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-03-24T11:32:03-0400</dcterms:modified>
